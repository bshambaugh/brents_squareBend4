FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField uniform 2180.89;

boundaryField
{
 inletFace
    {
        type           zeroGradient;
    }
    inletWall
    {
        type            epsilonWallFunction;
        value           uniform 2180.89;
    }
    inletsourceFace
    {
        type            zeroGradient;
    }
    inletdestinationFace
    {
        type            zeroGradient;
    }
    fineMeshWall
    {
        type            epsilonWallFunction;
        value           uniform 2180.89;
    }
    fineMeshSourceFace
    {
        type            zeroGradient;
    }
    fineMeshdestinationFace
    {
        type            zeroGradient;
    }
    fineMeshSymmetryWall
    {
        type            symmetryPlane;
    }
    fineMeshOutletLeft
    {
        type            zeroGradient;
    }
    fineMeshOutletBottom
    {
        type           zeroGradient;
    }

}


